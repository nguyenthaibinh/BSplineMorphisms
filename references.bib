	Doi = {10.1016/j.neuroimage.2007.09.031},
	Journal = {Neuroimage},
	Journal-Full = {NeuroImage},
	Mesh = {Adolescent; Adult; Algorithms; Atlases as Topic; Brain Mapping; Cerebral Cortex; Echo-Planar Imaging; Humans; Image Processing, Computer-Assisted; Likelihood Functions; Models, Statistical; Nonlinear Dynamics; Observer Variation; Reference Values},
